				{
					"id": "set-animation-speed",
					"objectClass": "Player_sprite",
					"sid": 465540743808949,
					"parameters": {
						"speed": "2"
					}
				},
				{
					"type": "script",
					"language": "javascript",
					"script": [
						"const SPEED = 100;",
						"const MARGIN = 80;",
						"",
						"const player = runtime.objects.Player_sprite.getFirstInstance();",
						"if (!player) return;",
						"",
						"const H = runtime.layout.height / 2;",
						"const x = player.x + 500;",
						"",
						"const TOP_Y = 0 + MARGIN;",
						"const BOT_Y = H - MARGIN;",
						"",
						"const minScaleY = 1.0;",
						"const maxScaleY = 2.5;",
						"",
						"const sTop = minScaleY + Math.random() * (maxScaleY - minScaleY);",
						"const sBot = minScaleY + Math.random() * (maxScaleY - minScaleY);",
						"",
						"const top = runtime.objects.Coral.createInstance(0, x, TOP_Y);",
						"",
						"if (top) {",
						"  top.animationSpeed = 0;",
						"  top.animationFrame = Math.floor(Math.random() * 4);",
						"",
						"  top.height = -top.height * sTop;",
						"",
						"  const b = top.behaviors.Bullet;",
						"  b.speed = SPEED;",
						"}",
						"",
						"const bot = runtime.objects.Coral.createInstance(0, x, BOT_Y);",
						"if (bot) {",
						"  bot.animationSpeed = 0;",
						"  bot.animationFrame = Math.floor(Math.random() * 4);",
						"  ",
						"  bot.height = bot.height * sBot;",
						"",
						"  const b = bot.behaviors.Bullet;",
						"  b.speed = SPEED;",
						"}"
					]
				}
			],
			"sid": 884896111873384
		},
